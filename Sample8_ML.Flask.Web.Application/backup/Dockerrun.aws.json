{
  "AWSEBDockerrunVersion": "1",
  "containerDefinitions": [
    {
      "name": "app",
      "image": "andrewodrain/web-applications:app-v0.0.2",
      "essential": true
    },
    {
      "name": "nginx",
      "image": "andrewodrain/web-applications:nginx-v0.2",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80
        }
      ],
      "links": [
        "app"
      ]
    }
  ]
}
